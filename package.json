{
  "name": "givemetime",
  "version": "0.0.1",
  "description": "Project to give some time to a R&D project suggested by a team member",
  "main": "src/index.jsx",
  "scripts": {
    "test": "./node_modules/.bin/eslint src --ext .js --ext .jsx",
    "db": "docker-compose -f server/docker-compose.yml up",
    "db:rm": "docker-compose -f server/docker-compose.yml rm",
    "db:_sqitch": "docker run -it --rm -v $(pwd):/project --workdir=/project -v ~/.sqitch:/root/.sqitch aleksandrvin/sqitch",
    "db:deploy": "npm run db:_sqitch --loglevel=silent -- deploy db:pg://$PGUSER:$PGPASSWORD@$PGHOST:$PGPORT/$PGDATABASE --verify",
    "db:revert": "npm run db:_sqitch --loglevel=silent -- revert db:pg://$PGUSER:$PGPASSWORD@$PGHOST:$PGPORT/$PGDATABASE --to @HEAD^ -y",
    "db:rebase": "npm run db:_sqitch --loglevel=silent -- rebase db:pg://$PGUSER:$PGPASSWORD@$PGHOST:$PGPORT/$PGDATABASE @HEAD^ -y",
    "db:test": "docker run --rm -v $(pwd)/server/sqitch/test/:/test digit/docker-pg-prove -h $PGHOST -p $PGPORT -u $PGUSER -w $PGPASSWORD -d $PGDATABASE -t '/test/*.sql'",
    "client": "./node_modules/.bin/webpack-dev-server --host localhost --open",
    "server": "./node_modules/.bin/postgraphql postgres://$PGUSER:$PGPASSWORD@$PGHOST:$PGPORT/$PGDATABASE --schema give_me_time --development",
    "build": "./node_modules/.bin/webpack ./webpack.config.js"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ericraffin/givemetime.git"
  },
  "author": "ericraffin",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ericraffin/givemetime/issues"
  },
  "homepage": "https://github.com/ericraffin/givemetime#readme",
  "devDependencies": {
    "babel-core": "^6.9.0",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-flow-comments": "^6.8.0",
    "babel-plugin-transform-runtime": "^6.9.0",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-es2015-loose": "^7.0.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "babel-preset-stage-1": "^6.5.0",
    "css-loader": "^0.23.1",
    "eslint": "^2.11.1",
    "eslint-plugin-react": "^5.1.1",
    "react-hot-loader": "^1.3.0",
    "style-loader": "^0.13.1",
    "webpack": "^1.13.1",
    "webpack-dev-server": "^1.14.1"
  },
  "babel": {
    "presets": [
      "es2015",
      "react"
    ]
  },
  "dependencies": {
    "material-ui": "^0.15.0",
    "postgraphql": "^1.5.0",
    "react": "^15.0.2",
    "react-dom": "^15.0.2",
    "react-google-login": "^2.0.0",
    "react-grid-layout": "^0.12.3",
    "react-redux": "^4.4.5",
    "react-router": "^2.4.1",
    "react-tap-event-plugin": "^1.0.0",
    "redux": "^3.5.2",
    "redux-thunk": "^2.1.0"
  }
}
